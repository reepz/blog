<h1>Local Weather</h1>
<div id="temperature">

</div>

<%= javascript_tag do %>
  var temperature;
  $.getJSON("https://fcc-weather-api.glitch.me/api/current?lat=52&lon=21?callback=?", function(json) {
    temperature = json.main.temp;
    $("#temperature").append(temperature);
    console.log(temperature);
  });
<% end %>
